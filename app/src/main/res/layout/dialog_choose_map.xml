<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/bg_scan_list_tl_tr_24dp">

    <TextView
        android:layout_marginTop="16dp"
        android:id="@+id/tv_location_name"
        android:layout_width="0dp"
        android:gravity="center"
        android:textSize="12sp"
        android:textColor="@android:color/black"
        android:layout_marginHorizontal="27dp"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:visibility="gone"
        android:id="@+id/tv_amap"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:paddingTop="13dp"
        android:paddingBottom="10dp"
        android:text="@string/a_map"
        android:textAlignment="center"
        android:textColor="@color/tv_color"
        android:textSize="15sp"
        app:layout_constraintBottom_toTopOf="@+id/view_line1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_location_name" />

    <View
        android:visibility="gone"
        android:id="@+id/view_line1"
        android:layout_width="0dp"
        android:layout_height="0.5dp"
        android:background="@color/v_line_color"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_amap" />

    <TextView
        android:visibility="gone"
        android:id="@+id/tv_baidu_map"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:paddingTop="13dp"
        android:paddingBottom="10dp"
        android:text="@string/baidu_map"
        android:textAlignment="center"
        android:textColor="@color/tv_color"
        android:textSize="15sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view_line1" />

    <View
        android:visibility="gone"
        android:id="@+id/view_line2"
        android:layout_width="0dp"
        android:layout_height="0.5dp"
        android:background="@color/v_line_color"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_baidu_map" />

    <TextView
        android:visibility="gone"
        android:id="@+id/tv_tencent_map"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:paddingTop="10dp"
        android:paddingBottom="15dp"
        android:text="@string/tencent_map"
        android:textAlignment="center"
        android:textColor="@color/tv_color"
        android:textSize="15sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view_line2" />
    <View
        android:visibility="gone"
        android:id="@+id/view_line3"
        android:layout_width="0dp"
        android:layout_height="0.5dp"
        android:background="@color/v_line_color"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_tencent_map" />

    <TextView
        android:visibility="gone"
        android:id="@+id/tv_google_map"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:paddingTop="10dp"
        android:paddingBottom="15dp"
        android:text="@string/google_map_text"
        android:textAlignment="center"
        android:textColor="@color/tv_color"
        android:textSize="15sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view_line3" />
    <View
        android:id="@+id/view_line4"
        android:layout_width="0dp"
        android:layout_height="0.5dp"
        android:background="@color/v_line_color"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_google_map" />
    <TextView
        android:id="@+id/tv_cancel"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:paddingTop="18dp"
        android:paddingBottom="18dp"
        android:text="@string/cancel"
        android:textAlignment="center"
        android:textColor="@color/tv_color"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_google_map" />

</androidx.constraintlayout.widget.ConstraintLayout>